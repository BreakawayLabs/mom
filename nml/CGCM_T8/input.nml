 &ocean_barotropic_nml
      ! vertically integrated dynamics 
      ! biharmonic : WARNING: This operator is NOT positive definite, and so can produce spurious extrema.
      ! use: implicit tidal forcing and smooth eta_t and pbot_t
      debug_this_module=.false.
      barotropic_halo=1				!barotropic_halo > 1 barotropic time step to improve the performance
      barotropic_time_stepping_A=.false.   	! old =.true.  eta_t and pbot_t updated with big time step 
      barotropic_time_stepping_B=.true.    	! old =.false.  eta_t and pbot_t updated with time average
      pred_corr_gamma=0.20			! dissipation parameter predictor-corrector. 0.0<val<0.25 is recomended
      smooth_eta_t_laplacian=.false.  		! old =.true.  depth-based coordinates. No need in Cgrid default=.true.
      smooth_pbot_t_laplacian=.true. 		! old =.true.  pbot_t-pbot0   PRESSURE-based coordinates default=.true. 
      smooth_eta_t_biharmonic=.false.  		! old =.false. only for depth-based coordinates. No need in Cgrid
      smooth_pbot_t_biharmonic=.false. 		! old =.false. pbot_t-pbot0   PRESSURE-based coordinates default=.false.
      smooth_eta_diag_laplacian=.true.		! old =.true.  PRESSURE-based coordinates
      vel_micom_lap_diag=1.0			! old =1.0 velocity scale in MICOM Laplacian mixing default=0.2
      vel_micom_lap=0.05	                ! old =.05 coeff to smooth eta_t and pbot_t default=0.05	
      truncate_eta=.true.			! old =.false. eta to be positive in top cell for zstar/pstar tides =.true.
      eta_max=8.0				! old =8.0 maximum positive eta_t allowed  default = 5.0
      verbose_truncate=.true.			! old =.true. printout on initialization
      diag_step=12				! old =12 Frequency for output of ascii barotropic diagnostics.
      tidal_forcing_m2=.false.			!Forces from lunar M2 tidal constituent.
      tidal_forcing_8=.true.			!Forces from 8 lunar and solar tidal constituents	
      tidal_forcing_ideal=.false.		!For ideal tidal forcing, which has a bump configuration.
      alphat=0.948				!Dimensionless self-attraction and loading term. only tidal_forcing=.true.	
      geoid_forcing=.false.			!For modifying the geoid as a time independent tidal forcing. need read a file
/

